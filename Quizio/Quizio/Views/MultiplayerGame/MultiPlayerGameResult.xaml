<UserControl x:Class="Quizio.Views.MultiplayerGame.MultiplayerGameResult"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="700">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab SelectedSource="/Views/MultiplayerGame/Overview.xaml" Layout="Tab">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="übersicht" Source="/Views/MultiplayerGame/Overview.xaml"/>
                <mui:Link DisplayName="detaillierte resultate" Source="/Views/MultiplayerGame/SplitResult.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
